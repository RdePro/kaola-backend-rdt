{
  "name": "@rde-pro/vue-starter-rdt",
  "description": "rdt-vue-starter rede-template",
  "version": "0.0.2-beta.1",
  "keywords": [
    "@rde-pro/discovery"
  ],
  "license": "MIT",
  "files": [
    "/_docs",
    "/app",
    "/template",
    ".gitignore",
    "/package.json",
    "/rde.template.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "cd ./template && npm run lint",
    "release:beta": "npm version prerelease --preid=beta && npm publish",
    "release:pre": "npm version prerelease && npm publish",
    "release": "npm version patch && npm publish"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "validate-commit-msg": "^2.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "validate-commit-msg"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ],
    "*.vue": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {}
}
